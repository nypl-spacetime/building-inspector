<% content_for :page_title do %>Building Inspector by NYPL Labs<% end %>

<% content_for :dynamic_js do %>
  <%= stylesheet_link_tag    "fixer", :media => "all" %>

  <%= javascript_include_tag "inspector_address", {:id => "addressjs", "data-server" => CGI.escape("#{request.protocol}#{request.host_with_port}"), "data-session" => "#{@isNew}", "data-map" => "#{@map}"} %>
<% end %>

<%= render :partial => "partials/nav" %>
<%= render :partial => "partials/main_container" %>
<div id="controls">
  <div id="buttons">
    <div class="wrapper">
      <a href="javascript:;" id="submit-button" title="Keyboard shortcut: S" rel="nofollow">SKIP</a>
    </div>
  </div>
</div>
<%= link_to (content_tag "span","The New York Public Library"), "http://www.nypl.org", {:class => "nypl-logo", :title => "Visit NYPL.org", :id=>"nypl-mini"} %>
